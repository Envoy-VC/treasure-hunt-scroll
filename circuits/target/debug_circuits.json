n":{"start":2473,"end":2504},"file":0}],"373":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"374":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"375":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"376":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"377":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"378":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"379":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"380":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"381":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"382":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"383":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"384":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"385":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"386":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"387":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"388":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"389":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"390":[{"span":{"start":230,"end":271},"file":1},{"span":{"start":2473,"end":2504},"file":0}],"391":[{"span":{"start":284,"end":298},"file":1}]}}],"file_map":{"0":{"source":"use dep::std;\n\nstruct Riddle {\n    question: [u8; 32],\n    answerHash: [u8; 32],\n    position: [Field; 2],\n}\n\npub fn getRiddles() -> [Riddle; 3] {\n    let riddle = Riddle { question: [0; 32], answerHash: [0; 32], position: [0; 2] };\n\n    let mut riddles: [Riddle; 3] = [riddle; 3];\n\n    /*\n    I am the block number on the Ethereum blockchain where The DAO incident took place. I caused a significant community discussion and a subsequent fork. Which block number am I?\n\n    1920000\n\n    [26,32]\n    */\n\n    riddles[0] = Riddle {\n        question: getHash(\"I am the block number on the Ethereum blockchain where The DAO incident took place. I caused a significant community discussion and a subsequent fork. Which block number am I?\".as_bytes()),\n        answerHash: getHash(\"1920000\".as_bytes()),\n        position:[26,32]\n    };\n\n    /*\n        I am a consensus algorithm introduced to Ethereum to replace proof-of-work. I involve validators who lock up a certain amount of cryptocurrency to validate blocks. What is my name?\n\n        Casper\n\n        [32, 28]\n    */\n\n    riddles[1] = Riddle {\n        question: getHash(\"I am a consensus algorithm introduced to Ethereum to replace proof-of-work. I involve validators who lock up a certain amount of cryptocurrency to validate blocks. What is my name?\".as_bytes()),\n        answerHash: getHash(\"Casper\".as_bytes()),\n        position: [32, 28]\n    };\n\n    /*\n        In 2014, I became the first cryptocurrency to implement the X11 hashing algorithm, aiming for increased security and energy efficiency. What is my name?\n\n        Dash\n\n        [4,2]\n    */\n\n    riddles[2] = Riddle {\n        question: getHash(\"In 2014, I became the first cryptocurrency to implement the X11 hashing algorithm, aiming for increased security and energy efficiency. What is my name?\".as_bytes()),\n        answerHash: getHash(\"Dash\".as_bytes()),\n        position: [4,2]\n    };\n\n    riddles\n}\n\npub fn getRiddleForPos(pos: [Field; 2]) -> Riddle {\n    let riddles = getRiddles();\n\n    let mut riddle = Riddle { question: [0; 32], answerHash: [0; 32], position: [0; 2] };\n\n    for i in 0..3 {\n        if {\n            riddles[i].position == pos\n        } {\n            riddle.question = riddles[i].question;\n            riddle.answerHash = riddles[i].answerHash;\n            riddle.position = riddles[i].position;\n        }\n    }\n\n    riddle\n}\n\npub fn solveRiddle(riddle: Riddle, answerHash: [u8; 32]) -> bool {\n    let mut solved = false;\n\n    if riddle.answerHash == answerHash {\n        solved = true;\n    }\n    solved\n}\n\npub fn getHash<N>(data: [u8; N]) -> [u8; 32] {\n    std::hash::keccak256(data, N)\n}\n","path":"/Users/envoy1084/Projects/Personal/treasure-hunt-scroll/circuits/src/treasure.nr"},"1":{"source":"mod treasure;\nuse dep::std;\n\nfn main(position: pub [Field; 2], answerHash: [u8; 32]) {\n    let riddle = treasure::getRiddleForPos(position);\n    assert(riddle.position != [0, 0], \"No Riddle found for position\");\n\n    let result = treasure::solveRiddle(riddle, answerHash);\n    assert(result == true, \"Riddle not solved\");\n}\n\n#[test]\nfn test_correct() {\n    let position = [32, 28];\n    let answerHash = treasure::getHash(\"Casper\".as_bytes());\n    main(position, answerHash);\n}\n\n#[test(should_fail)]\nfn test_wrong() {\n    let position = [0, 0];\n    let answerHash = treasure::getHash(\"Wrong\".as_bytes());\n    main(position, answerHash);\n}\n","path":"/Users/envoy1084/Projects/Personal/treasure-hunt-scroll/circuits/src/main.nr"}},"warnings":[]}